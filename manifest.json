{
    "manifest_version": 3,
    "name": "Fullscreen",
    "version": "0.0.3",
    "description": "Fullscreen extension for device without keyboard",
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_title": "Click to fullscreen"
    },
    "background":
    {
        "service_worker": "main.js"
    }
}
